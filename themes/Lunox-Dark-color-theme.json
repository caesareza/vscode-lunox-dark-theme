{
  "name": "Lunox Dark",
  "colors": {
    "breadcrumb.foreground": "#73878C",
    "debugIcon.breakpointForeground": "#FF499E",
    // "sideBar.border": "#384447",
    // "editorGroupHeader.tabsBorder": "#384447",
    "editorGroup.emptyBackground": "#1A1E1E",
    "editorGroup.dropIntoPromptBackground": "#1A1E1E",
    // "editorGroup.border": "#1A1E1E",
    "editorGroupHeader.noTabsBackground": "#1A1E1E",
    "editorGroupHeader.tabsBackground": "#1F2428",
    "editor.background": "#1A1E23",
    "editor.foreground": "#ccc",
    "sideBarTitle.foreground": "#738c88",
    "statusBar.noFolderForeground": "#73878C",
    "statusBar.noFolderBackground": "#23292b",
    "statusBar.background": "#1A1E23",
    // "statusBar.border": "#384447",
    "statusBar.foreground": "#92ada8",
    "sideBar.background": "#1F2428",
    "sideBar.foreground": "#E5E7EB",
    "sideBar.dropBackground": "#C94370",
    // "sideBarSectionHeader.border": "#3f4b4e",
    "sideBarSectionHeader.foreground": "#ccc",
    "sideBarSectionHeader.background": "#1F2428",
    "activityBar.background": "#1F2428",
    "activityBar.activeBorder": "#597c82",
    // ffafcc 8fc0a9
    "activityBar.foreground": "#6e9887",
    // "activityBar.border": "#384447",
    "activityBarBadge.background": "#C94370",
    "titleBar.activeBackground": "#1F2428",
    // "titleBar.border": "#384447",
    "titleBar.inactiveForeground": "#FF499E",
    "editorLineNumber.foreground": "#3B4548",
    "editorLineNumber.activeForeground": "#73878C",
    "list.activeSelectionBackground": "#23292b",
    "list.activeSelectionForeground": "#FF499E",
    "list.activeSelectionIconForeground": "#62a8ac",
    "gitDecoration.modifiedResourceForeground": "#b2cac1",
    "gitDecoration.untrackedResourceForeground": "#415d43",
    "gitDecoration.ignoredResourceForeground": "#415d43",
    "gitDecoration.addedResourceForeground": "#FF499E",
    "gitDecoration.stageModifiedResourceForeground": "#FF499E",
    "gitDecoration.renamedResourceForeground": "#FF499E",
    // "tab.activeBorder": "#FF499E",
    "tab.inactiveBackground": "#1F2428",
    "terminal.foreground": "#597c82",
    "list.focusOutline": "#FF499E",
    "button.background": "#597c82",
    "textLink.foreground": "#FF499E"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#556268"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#efefef"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#efefef"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        // fe621d
        "foreground": "#FF3471"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "constant.other.color",
        "punctuation",
        "meta.tag",
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#ff499e"
      }
    },
    {
      "name": "keyword control",
      "scope": ["keyword.control"],
      "settings": {
        "foreground": "#bddd3d",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Other",
      "scope": [
        "constant.other.color",
        "meta.tag",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        // ffccd5  aeaeae
        "foreground": "#d1d1d1"
      }
    },
    {
      "name": "punctuation Tag Bedin End",
      "scope": [
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded"
      ],
      "settings": {
        // ffccd5  aeaeae
        "foreground": "#c0586b"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#c94370"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#56ab91"
      }
    },
    {
      "name": "Block Level Variables",
      "scope": ["meta.block variable.other"],
      "settings": {
        "foreground": "#efefef"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": ["support.other.variable", "string.other.link"],
      "settings": {
        "foreground": "#219ebc"
      }
    },
    {
      "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape",
        "variable.parameter",
        "keyword.other.unit",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#a0d974"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "string",
        "punctuation",
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "foreground": "#a0d974"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name",
        "support.type",
        "support.class",
        "support.other.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "support.type.sys-types"
      ],
      "settings": {
        // F75590 c94370
        "foreground": "#c94370"
      }
    },
    {
      "name": "Entity Types",
      "scope": ["support.type"],
      "settings": {
        "foreground": "#efefef"
      }
    },
    {
      "name": "CSS Class and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#efefef"
      }
    },
    {
      "name": "Sub-methods",
      "scope": [
        "entity.name.module.js",
        "variable.import.parameter.js",
        "variable.other.class.js"
      ],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Language methods",
      "scope": ["variable.language"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FF5370"
      }
    },
    {
      "name": "entity.name.method.js",
      "scope": ["entity.name.method.js"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "meta.method.js",
      "scope": [
        "meta.class-method.js entity.name.function.js",
        "variable.function.constructor"
      ],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        // fe7295
        "foreground": "#ff92b1"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "text.html.basic entity.other.attribute-name.html",
        "text.html.basic entity.other.attribute-name"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#90be6d"
      }
    },
    {
      "name": "CSS Classes",
      "scope": ["entity.other.attribute-name.class"],
      "settings": {
        "foreground": "#90be6d"
      }
    },
    {
      "name": "CSS ID's",
      "scope": ["source.sass keyword.control"],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#C3E88D"
      }
    },
    {
      "name": "Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "tag.decorator.js entity.name.tag.js",
        "tag.decorator.js punctuation.definition.tag.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "ES7 Bind Operator",
      "scope": [
        "source.js constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FF5370"
      }
    },
